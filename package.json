{
  "name": "@dekzer/kissfft",
  "version": "0.1.0",
  "description": "WebAssembly-compiled KISS FFT library with TypeScript bindings",
  "keywords": [
    "fft",
    "webassembly",
    "dsp",
    "signal processing",
    "kissfft"
  ],
  "type": "module",
  "homepage": "https://github.com/dekzer-oss/kissfft#readme",
  "bugs": {
    "url": "https://github.com/dekzer-oss/kissfft/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dekzer-oss/kissfft.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./wasm": "./build/kissfft-wasm.js"
  },
  "scripts": {
    "prepare": "bash scripts/build-wasm.sh && tsc -p tsconfig.json",
    "build": "bash scripts/build-wasm.sh && tsc",
    "test": "vitest --reporter=verbose",
    "bench": "vitest bench --reporter=verbose",
    "postinstall": "npm run build",
    "prepublishOnly": "npm run build && npm run test",
    "format": "prettier --write ."
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "prettier": "^4.0.0-alpha.12",
    "rollup-plugin-copy": "^3.5.0",
    "vite": "^7.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
